<?xml version="1.0"?>
<launch>
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find vertical_control)/worlds/orange_target.world"/>
  </include>

  <!-- Spawn turtlebot -->
  <group ns="turtlebot">
  <include file="$(find vertical_control)/launch/spawn_turtlebot.launch" >
  </include>
  </group>

  <!-- Spawn simulated quadrotor uav -->
  <group ns="quadcopter">
  <include file="$(find vertical_control)/launch/spawn_quadrotor.launch" >
    <arg name="model" value="$(find cvg_sim_gazebo)/urdf/quadrotor_sensors.urdf.xacro"/>
  </include>
  </group>




</launch>
